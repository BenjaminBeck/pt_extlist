									<DATATYPE>Associative array</DATATYPE>
									<POSIBLEVALUES>All columnIdentifiers</POSIBLEVALUES>
									<DEFAULT></DEFAULT>
									<STDWRAP></STDWRAP>
									<PROTOTYPE></PROTOTYPE>
									<EXAMPLE></EXAMPLE>
									<CHILDREN>
										<ENTRY KEY="[yourColumnId]">
											<DESCRIPTION></DESCRIPTION>
											<DATATYPE></DATATYPE>
											<POSIBLEVALUES></POSIBLEVALUES>
											<DEFAULT></DEFAULT>
											<STDWRAP></STDWRAP>
											<PROTOTYPE></PROTOTYPE>
