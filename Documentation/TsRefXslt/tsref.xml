												</ENTRY>
												<ENTRY KEY="sortingImageAsc">
													<DESCRIPTION></DESCRIPTION>
													<PROTOTYPE></PROTOTYPE>
													<STDWRAP>0</STDWRAP>
													<DATATYPE></DATATYPE>
													<EXAMPLE></EXAMPLE>
												</ENTRY>
												<ENTRY KEY="sortingImageDesc">
													<DESCRIPTION></DESCRIPTION>
													<PROTOTYPE></PROTOTYPE>
													<STDWRAP>0</STDWRAP>
													<DATATYPE></DATATYPE>
													<EXAMPLE></EXAMPLE>
												</ENTRY>
												<ENTRY KEY="sortingImageDefault">
													<DESCRIPTION></DESCRIPTION>
													<PROTOTYPE></PROTOTYPE>
													<STDWRAP>0</STDWRAP>
													<DATATYPE></DATATYPE>
													<EXAMPLE></EXAMPLE>
												</ENTRY>
												<ENTRY KEY="accessGroups">
													<DESCRIPTION></DESCRIPTION>
													<PROTOTYPE></PROTOTYPE>
													<STDWRAP>0</STDWRAP>
