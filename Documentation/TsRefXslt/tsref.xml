													<DESCRIPTION></DESCRIPTION>
													<DATATYPE></DATATYPE>
													<POSIBLEVALUES></POSIBLEVALUES>
													<DEFAULT></DEFAULT>
													<STDWRAP>0</STDWRAP>
													<PROTOTYPE></PROTOTYPE>
													<EXAMPLE></EXAMPLE>
												</ENTRY>
												<ENTRY KEY="special">
													<DESCRIPTION>Insert a individual SQL snippet.</DESCRIPTION>
													<DATATYPE></DATATYPE>
													<POSIBLEVALUES></POSIBLEVALUES>
													<DEFAULT></DEFAULT>
													<STDWRAP>0</STDWRAP>
													<PROTOTYPE></PROTOTYPE>
													<EXAMPLE></EXAMPLE>
												</ENTRY>
												<ENTRY KEY="isSortable">
													<DESCRIPTION></DESCRIPTION>
													<DATATYPE></DATATYPE>
													<POSIBLEVALUES></POSIBLEVALUES>
													<DEFAULT></DEFAULT>
													<STDWRAP>0</STDWRAP>
													<PROTOTYPE></PROTOTYPE>
													<EXAMPLE></EXAMPLE>
												</ENTRY>
												<ENTRY KEY="expandGroupRows">
													<DESCRIPTION></DESCRIPTION>
													<DATATYPE></DATATYPE>
													<POSIBLEVALUES></POSIBLEVALUES>
													<DEFAULT></DEFAULT>
													<STDWRAP>0</STDWRAP>
													<PROTOTYPE></PROTOTYPE>
													<EXAMPLE></EXAMPLE>
												</ENTRY>
											</CHILDREN>
										</ENTRY>
									</CHILDREN>
								</ENTRY>
								<ENTRY KEY="columns">
									<DESCRIPTION>Holds the tables column definitions.</DESCRIPTION>
									<PROTOTYPE></PROTOTYPE>
									<STDWRAP>0</STDWRAP>
									<DATATYPE>Array</DATATYPE>
									<EXAMPLE></EXAMPLE>
									<CHILDREN>
										<ENTRY KEY="10,20,30">
											<CHILDREN>
												<ENTRY KEY="fieldIdentifier">
													<DESCRIPTION></DESCRIPTION>
													<PROTOTYPE></PROTOTYPE>
													<STDWRAP>0</STDWRAP>
													<DATATYPE></DATATYPE>
													<EXAMPLE></EXAMPLE>
												</ENTRY>
												<ENTRY KEY="label">
													<DESCRIPTION>A label for this element.</DESCRIPTION>
													<PROTOTYPE></PROTOTYPE>
													<STDWRAP>1</STDWRAP>
													<DATATYPE>String</DATATYPE>
													<EXAMPLE></EXAMPLE>
												</ENTRY>
												<ENTRY KEY="renderUserFunctions">
													<DESCRIPTION>A list of userfunctions to render the field value.</DESCRIPTION>
													<PROTOTYPE></PROTOTYPE>
													<STDWRAP>0</STDWRAP>
													<DATATYPE></DATATYPE>
													<EXAMPLE></EXAMPLE>
												</ENTRY>
												<ENTRY KEY="renderTemplate">
													<DESCRIPTION></DESCRIPTION>
													<PROTOTYPE></PROTOTYPE>
													<STDWRAP>0</STDWRAP>
													<DATATYPE></DATATYPE>
													<EXAMPLE></EXAMPLE>
												</ENTRY>
												<ENTRY KEY="renderObj">
													<DESCRIPTION></DESCRIPTION>
													<PROTOTYPE></PROTOTYPE>
													<STDWRAP>0</STDWRAP>
													<DATATYPE></DATATYPE>
